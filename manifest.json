{
          "manifest_version": 3,
          "name": "Clear Site Data",
          "version": "1.0",
          "description": "Clears all site data for the current website and reloads the page",
          "permissions": [
                    "activeTab",
                    "cookies",
                    "storage",
                    "browsingData"
          ],
          "host_permissions": [
                    "<all_urls>"
          ],
          "action": {
                    "default_icon": {
                              "16": "icon16.png",
                              "48": "icon48.png",
                              "128": "icon128.png"
                    }
          },
          "background": {
                    "service_worker": "background.js"
          }
}
